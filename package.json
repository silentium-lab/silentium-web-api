{
  "name": "silentium-web-api",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "main": "dist/silentium-web-api.js",
  "module": "dist/silentium-web-api.mjs",
  "typings": "dist/silentium-web-api.d.ts",
  "keywords": [
    "oop",
    "web-api"
  ],
  "scripts": {
    "build": "rollup -c",
    "build-doc": "./docs/build.sh",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "test": "vitest",
    "cz": "git add . && git cz",
    "push-head": "git push origin HEAD",
    "test-debug": "env DEBUG=app:* vitest",
    "release": "./beforeRelease.sh && npm run build && standard-version --no-verify && git push --follow-tags && npm publish",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/silentium-lab/silentium-web-api.git"
  },
  "private": false,
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cz-customizable": "^7.0.0",
    "esbuild": "^0.25.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "prettier": "3.3.3",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "standard-version": "^9.5.0",
    "vitest": "^3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.cjs"
    }
  },
  "dependencies": {
    "silentium": "^0.0.2"
  }
}
